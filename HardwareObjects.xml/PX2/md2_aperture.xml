<device class="TangoMotorMD2PhasesWPositionsPX2">
    <username>MD2 Aperture</username>
    <tangoname>i11-ma-cx1/ex/md2</tangoname> <!--check!-->
    <channel type="tango" polling="1000" name="state">State</channel>
    <channel type="tango" tangoname="i11-ma-cx1/ex/md2" polling="1000" name="position">AperturePredefinedPosition</channel>
    <delta>0.1</delta>
    <interval>1000</interval>
    <positions>
        <position>
            <username>On</username>
            <offset>1</offset>
        </position>
        <position>
            <username>Off</username>
            <offset>2</offset>
        </position>
        <!--<position>
            <username>3</username>
            <offset>3</offset>
        </position>-->
        <position>
            <username>Cover</username>
            <offset>4</offset>
        </position>
        <!--<position>
            <username>Transfer</username>
            <offset>5</offset>
        </position>-->
    </positions>
</device>
