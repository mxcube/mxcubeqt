<equipment class="MiniDiff">
    <username>Goniometer</username>
    
    <referencePosition>
        <phi delta="0.1">0.0</phi>
        <phiz delta="0.01">0.0</phiz>
        <zoom delta="0.01"></zoom>
        <light delta="0.01"></light>
        <focus delta="0.01">0.0</focus>
        <phiy delta="0.01">0.0</phiy>
        <sampx delta="0.01">0.0</sampx>
        <sampy delta="0.01">0.0</sampy>
    </referencePosition>
    <referencePositionWithKappa>
        <phi delta="0.1">0.0</phi>
        <phiz delta="0.01">0.0</phiz>
        <zoom delta="0.01"></zoom>
        <light delta="0.01"></light>
        <focus delta="0.01">0.0</focus>
        <phiy delta="0.01">0.0</phiy>
        <sampx delta="0.01">0.0</sampx>
        <sampy delta="0.01">0.0</sampy>
        <kap1 delta="0.01">0.0</kap1>
        <kap2 delta="0.01">0.0</kap2>
    </referencePositionWithKappa>
    <resetPosition>
        <phiz>0.0</phiz>
        <focus>0.0</focus>
        <phiy>0.0</phiy>
        <sampx>0.0</sampx>
        <sampy>0.0</sampy>
    </resetPosition>
    <device role="camera" hwrid="/ge1350c"></device>
    <motors>
        <phi-shutter>
            <device role="phi" hwrid="/tangoPhi"></device>
            <device role="omega" hwrid="/tangoOmega"></device>
            <device role="kappa" hwrid="/tangoKappa"></device>
            <device role="phiz" hwrid="/phiz"></device>  
        </phi-shutter>
        <sample>
            <device role="zoom" hwrid="/zoom"></device>
            <!--device role="light" hwrid="/light"></device-->
            <!--device role="focus" hwrid="/phix"></device-->
            <device role="phiy" hwrid="/mscPhiy"></device>  
            <device role="sampx" hwrid="/mscSampx"></device>
            <device role="sampy" hwrid="/mscSampy"></device>
            <device role="beamcenterx" hwrid="/beamcenterX"></device>
            <device role="beamcenterz" hwrid="/beamcenterZ"></device>
        </sample>
    </motors>
    <wagolight>/lightarm</wagolight>
</equipment>
