<device class="TangoLightPX1">
    <username>Light Arm</username>
    <offset>0</offset>
    <channel type="tango" name="attributeName" tangoname="i10-c-cx1/ex/light.1-pos" polling="1000">isInserted</channel>
    <channel type="tango" name="beamstopState" tangoname="i10-c-cx1/ex/bst.1-ev" polling="1000">State</channel>
    <command type="tango" tangoname="i10-c-cx1/ex/light.1-pos" name="set_in">Insert</command>
    <command type="tango" tangoname="i10-c-cx1/ex/light.1-pos" name="set_out">Extract</command>
    <command type="tango" tangoname="i10-c-cx1/ex/bst.1-ev" name="beamstop_out">Extract</command>
    <channel type="tango" name="detDistance" tangoname="i10-c-cx1/dt/dtc_se.1-mt_ts" polling="1000">position</channel>
    <min_detector_distance>269.5</min_detector_distance>
   <px1environment_dev>i10-c-cx1/ex/px1environment</px1environment_dev>
   <light_dev>i10-c-cx1/ex/light.1</light_dev>
</device>







