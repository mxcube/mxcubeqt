  <beaminfo>/beam-info</beaminfo>

  <zoomCentre>{"x": 680,"y": 512}</zoomCentre>
  <omegaReference>{"motor_name": "phiz", "position":-0.0175, "camera_axis":"y","direction": -1}</omegaReference>
  <gridDirection>{"fast": (1, 0), "slow": (0, -1)}</gridDirection>
  <reversingRotation>True</reversingRotation>
  <phaseList>("Transfer", "Centring", "DataCollection", "BeamLocation")</phaseList>
