<device class="MicrodiffMotor">
    <tangoname>i11-ma-cx1/ex/md2</tangoname>
    <motor_name>CapillaryVertical</motor_name>
    <username>CapillaryVertical</username>
    <direction>1</direction>
    <GUIstep>0.05</GUIstep>
    <channel type="tango" polling="1000" name="CapillaryVerticalPosition">CapillaryVerticalPosition</channel>
    <channel type="tango" polling="1000" name="CapillaryVerticalState">CapillaryVerticalState</channel>
    <channel type="tango" polling="1000" name="motor_states">MotorStates</channel>
    <command type="tango" name="abort">abort</command>
    <command type="tango" name="homing">startHomingMotor</command>
    <command type="tango" name="getCapillaryVerticalDynamicLimits">getMotorDynamicLimits</command>
    <command type="tango" name="get_limits">getMotorLimits</command>
    <command type="tango" name="get_max_speed">getMotorMaxSpeed</command>
</device>