    <motor_name>BeamstopX</motor_name>
    <username>BeamstopX</username>
    <direction>1</direction>
    <GUIstep>0.1</GUIstep>
    <channel type="tango" polling="1000" name="BeamstopXPosition">BeamstopXPosition</channel>
    <channel type="tango" polling="1000" name="BeamstopXState">BeamstopXState</channel>
