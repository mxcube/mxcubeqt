<!-- <?xml version="1.0" encoding="iso-8859-1"?> -->
<equipment class="Microdiff"> <!-- "MicrodiffPX2" -->
    <username>Goniometer</username>
    <referencePosition>
        <phi delta="0.1">0.0</phi>
        <sampx delta="0.01">0.0</sampx>
        <sampy delta="0.01">0.0</sampy>
        <phiy delta="0.01">0.0</phiy>
        <phiz delta="0.01">0.0</phiz>
        <zoom delta="0.01"></zoom>
        <wagolight delta="0.01"></wagolight>
        <focus delta="0.01">0.0</focus>
    </referencePosition>
    <resetPosition>
        <focus>0.0</focus>
        <phiz>0.0</phiz>
        <phiy>0.0</phiy>
        <sampx>0.0</sampx>
        <sampy>0.0</sampy>
    </resetPosition>
    <device role="camera" hwrid="/gc655c"></device>
    <motors>
        <phi-shutter>
            <device role="phi" hwrid="/phi"></device>
            <device role="sampx" hwrid="/sampx"></device> <!-- mxCuBE did not like those -->
            <device role="sampy" hwrid="/sampy"></device> <!-- two lines moved to <sample> section -->
        </phi-shutter>
        <sample>
            <device role="zoom" hwrid="/zoom"></device>
            <device role="focus" hwrid="/focus"></device>
            <device role="phiy" hwrid="/horizontal"></device>
            <device role="phiz" hwrid="/vertical"></device>
            <device role="light" hwrid="/light"></device>
            <!--<device role="beamcenterx" hwrid="/beamcenterX"></device>
            <device role="beamcenterz" hwrid="/beamcenterZ"></device>-->
        </sample>
    </motors>
    <samplechanger>/cats</samplechanger>
    <wagolight>/wago_light</wagolight>
</equipment>
