<device class="MicrodiffMotor">
    <tangoname>i11-ma-cx1/ex/md2</tangoname>
    <motor_name>Omega</motor_name>
    <username>Omega</username>
    <direction>-1</direction>
    <GUIstep>45</GUIstep>
    <channel type="tango" polling="events" name="OmegaPosition">OmegaPosition</channel>
    <channel type="tango" polling="events" name="OmegaState">OmegaState</channel>
    <channel type="tango" polling="events" name="motor_states">MotorStates</channel>
    <command type="tango" polling="events" name="abort">abort</command>
    <command type="tango" polling="events" name="homing">startHomingMotor</command>
    <command type="tango" polling="events" name="getOmegaDynamicLimits">getMotorDynamicLimits</command>
    <command type="tango" polling="events" name="get_limits">getMotorLimits</command>
    <command type="tango" polling="events" name="get_max_speed">getMotorMaxSpeed</command>
</device>