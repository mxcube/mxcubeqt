    <motor_name>AlignmentY</motor_name>
    <username>AlignmentY</username>
    <direction>1</direction>
    <GUIstep>0.1</GUIstep>
    <channel type="tango" polling="events" name="AlignmentYPosition">AlignmentYPosition</channel>
    <channel type="tango" polling="events" name="AlignmentYState">AlignmentYState</channel>
