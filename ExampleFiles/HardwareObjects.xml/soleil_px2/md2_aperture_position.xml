<!-- md2-zoomlevel has zoomlevel, backlightlevel and the frontlinelevel but not pixelsize and beamposition -->
<!-- <?xml version="1.0" encoding="iso-8859-1"?> -->
<device class="TangoMotorMD2PhasesWPositionsPX2">
    <username>Aperture On/Off/Out</username>
    <tangoname>i11-ma-cx1/ex/md2</tangoname>
    <channel type="tango" polling="1000" name="state">State</channel>
    <channel type="tango" tangoname="i11-ma-cx1/ex/md2" polling="1000" name="position">AperturePredefinedPosition</channel>
    <delta>0.1</delta>
    <interval>1000</interval>
    <positions>
        <position>
            <username>OnBeam</username>
            <offset>1</offset>
        </position>
        <position>
            <username>OffScreen</username>
            <offset>2</offset>
        </position>
        <position>
            <username>UnderCover</username>
            <offset>4</offset>
        </position>
    </positions>
</device>
